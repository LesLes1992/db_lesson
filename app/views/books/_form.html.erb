<%= form_with(model: @book) do |f| %>
    <fieldset class = "mb-3">
        <%= f.label :title %>
        <%= f.text_field :title%>
    </fieldset>
    <fieldset class = "mb-3">
        <%= f.label :author_id %>
        <%= f.collection_select :author_id, @author, :id, :full_name %>
    </fieldset>
    <fieldset class = "mb-3">
        <%= f.submit %>
    </fieldset>
<% end %>